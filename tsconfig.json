{
  "compilerOptions": {
     "lib": [
        "es5",
        "es6",
        "DOM",
        "DOM.Iterable"
     ],
     "target": "ES2017",
     "module": "commonjs",
     "moduleResolution": "node",
     "moduleDetection": "force",
     "outDir": "./build",
     "emitDecoratorMetadata": true,
     "experimentalDecorators": true,
     "sourceMap": true,
     "jsx": "preserve",
  }
}